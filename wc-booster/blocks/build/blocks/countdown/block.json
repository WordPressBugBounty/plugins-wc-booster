{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wc-booster/countdown",
  "version": "1.3",
  "title": "Countdown",
  "description": "The Countdown block highlights key features, designed for use on the product sale expiry date.",
  "attributes": {
    "block_id": {
      "type": "string"
    },
    "padding": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "isLinkActive": false,
        "properties": [
          "top",
          "right",
          "bottom",
          "left"
        ],
        "responsiveViews": [
          "desktop",
          "tablet",
          "mobile"
        ],
        "units": [
          "px",
          "em"
        ],
        "values": {
          "desktop": [
            "1",
            "1",
            "1",
            "1"
          ],
          "tablet": [
            "1",
            "1",
            "1",
            "1"
          ],
          "mobile": [
            "1",
            "1",
            "1",
            "1"
          ]
        }
      }
    },
    "margin": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "isLinkActive": false,
        "properties": [
          "top",
          "right",
          "bottom",
          "left"
        ],
        "responsiveViews": [
          "desktop",
          "tablet",
          "mobile"
        ],
        "units": [
          "px",
          "em"
        ],
        "values": {
          "desktop": [
            "1",
            "1",
            "1",
            "1"
          ],
          "tablet": [
            "1",
            "1",
            "1",
            "1"
          ],
          "mobile": [
            "1",
            "1",
            "1",
            "1"
          ]
        }
      }
    },
    "boxWidth": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "units": [
          "px",
          "em"
        ],
        "range": {
          "min": 1,
          "max": 1000
        },
        "values": {
          "desktop": 75,
          "tablet": 75,
          "mobile": 75
        }
      }
    },
    "boxHeight": {
      "type": "object",
      "default": {
        "activeUnit": "px",
        "units": [
          "px",
          "em"
        ],
        "range": {
          "min": 1,
          "max": 1000
        },
        "values": {
          "desktop": 70,
          "tablet": 70,
          "mobile": 70
        }
      }
    },
    "gap": {
      "type": "number",
      "default": 5
    },
    "alignment": {
      "type": "string",
      "default": "center"
    },
    "numberTypo": {
      "type": "object",
      "default": {
        "fontFamily": "",
        "fontSize": {
          "units": [
            "px",
            "em",
            "rem"
          ],
          "activeUnit": "px",
          "values": {
            "desktop": 22,
            "tablet": 20,
            "mobile": 20
          }
        },
        "fontWeight": 600,
        "lineHeight": {
          "activeUnit": "",
          "units": [
            ""
          ],
          "values": {
            "desktop": 1.2,
            "tablet": 1.2,
            "mobile": 1.2
          }
        }
      }
    },
    "labelTypo": {
      "type": "object",
      "default": {
        "fontFamily": "",
        "fontSize": {
          "units": [
            "px",
            "em",
            "rem"
          ],
          "activeUnit": "px",
          "values": {
            "desktop": 16,
            "tablet": 14,
            "mobile": 14
          }
        },
        "fontWeight": 400,
        "lineHeight": {
          "activeUnit": "",
          "units": [
            ""
          ],
          "values": {
            "desktop": 1.2,
            "tablet": 1.2,
            "mobile": 1.2
          }
        }
      }
    },
    "bgColor": {
      "type": "string",
      "default": "#f8f8f8"
    },
    "numColor": {
      "type": "string",
      "default": "#333"
    },
    "labelColor": {
      "type": "string",
      "default": "#777"
    },
    "targetDate": {
      "type": "string",
      "default": "2025-01-01T00:00:00"
    },
    "boxRadius": {
      "type": "number",
      "default": 0
    }
  },
  "textdomain": "wc-booster",
  "editorScript": "file:./index.js",
  "viewScript": "file:./countdown.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}